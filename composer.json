{
    "name": "madalinignisca/olx-api",
    "type": "library",
    "description": "OLX Api",
    "keywords": [
        "madalinignisca",
        "olx-api"
    ],
    "homepage": "https://github.com/madalinignisca/olx-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mădălin Ignișca",
            "email": "hi@madalin.me",
            "homepage": "https://madalin.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MadalinIgnisca\\Olx\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadalinIgnisca\\Olx\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
